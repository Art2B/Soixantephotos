extends partials/layout

block content
  h1= title
  p Welcome to #{title}
  p There is #{photos.length} photos actually.
  if photos.length > 0
    each photo in photos
      div.photo
        a(href="/photos/"+photo._id)
          img(src="/photos/"+photo._id, alt="")
        p= photo.name
        p= photo.category.name